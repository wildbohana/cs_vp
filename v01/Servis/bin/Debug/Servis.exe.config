<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>

	<system.serviceModel>
		<services>
			<service name="Servis.BibliotekaServis">
				<host>
					<baseAddresses>
						<add baseAddress="net.tcp://localhost:4002" />
					</baseAddresses>
				</host>
				
				<!-- Service Endpoints -->
				<endpoint address="ServisBiblioteka" 
						  binding="netTcpBinding" 
						  contract="Common.IBiblioteka" />
			</service>
		</services>
	</system.serviceModel>
</configuration>
